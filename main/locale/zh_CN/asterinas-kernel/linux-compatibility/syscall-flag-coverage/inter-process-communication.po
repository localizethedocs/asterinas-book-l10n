msgid ""
"// Block current thread if target value at `uaddr` matches `val`, and wait "
"up to `timeout`.\n"
msgstr ""

#: src/kernel/linux-compatibility/syscall-flag-coverage/inter-process-communication/index.md:23
msgid ""
"// Block current thread with bitmask condition if target value at `uaddr` "
"matches `val`,\n"
"// and wait up to `timeout`.\n"
msgstr ""

#: src/kernel/linux-compatibility/syscall-flag-coverage/inter-process-communication/index.md:31
msgid "// Unblock up to `max_waiters` threads waiting on `uaddr`\n"
msgstr ""

#: src/kernel/linux-compatibility/syscall-flag-coverage/inter-process-communication/index.md:38
